# Insyte.io Production Environment Settings
# Replace these values with your actual production settings

# Environment
ENVIRONMENT="production"

# Database - For render.com or similar
DATABASE_URL="postgresql://username:password@host:port/database"

# API Keys - Add your actual API keys
YOUTUBE_API_KEY="YOUR_YOUTUBE_API_KEY"
STRIPE_API_KEY="YOUR_STRIPE_API_KEY"
STRIPE_WEBHOOK_SECRET="YOUR_STRIPE_WEBHOOK_SECRET"
CALENDLY_API_KEY="YOUR_CALENDLY_API_KEY"
CALENDLY_WEBHOOK_SECRET="YOUR_CALENDLY_WEBHOOK_SECRET"
CALCOM_API_KEY="YOUR_CALCOM_API_KEY"

# Security - Generate a new secure key for production
SECRET_KEY="GENERATE_A_NEW_SECURE_KEY_HERE"

# URL Settings - Update with your actual deployed URLs
BASE_URL="https://insyte-io-api.onrender.com"
FRONTEND_URL="https://insyte-io.onrender.com"

# Logging
LOG_LEVEL="info"

# Scheduled task settings
YOUTUBE_REFRESH_INTERVAL="6"  # hours 